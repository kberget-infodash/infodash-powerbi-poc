    {
      "groupId": "5c03119e-3074-46fd-976b-c60198311f70", // Other
      "group": {
        "default": "Other"
      },
      "title": {
        "default": "Infodash Power BI"
      },
      "description": {
        "default": "The Infodash Power BI webpart is a proof-of-concept web part aimed to demonstate the ability to render a report and pass in environmental and user variables."
      },
      "officeFabricIconFontName": "BookmarkReport",
      "properties": {
        "height": "800",
        "reportOptions": {
          "hideFilterPane": true,
          "hideSlicer": false,
          "hidePageNavigation": false,
          "zoomLevel": 1
        }
      }
    }
  ]
}